{
  "name": "Ffuenf/Ffuenf_StockControl",
  "license": "MIT",
  "description": "This extension provides an easy-to-use admin grid displaying the important stock information for your store.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    },
    {
      "name": "Adam Moss"
    }
  ],
  "type": "magento-module",
  "require": {
    "aoepeople/composer-installers": "*",
    "colinmollenhour/modman": "*"
  },
  "require-dev": {
    "ecomdev/ecomdev_phpunit": "*",
    "phpunit/phpunit": "4.1.*",
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "map": [
      [
        "app/code/local/Ffuenf/StockControl",
        "app/code/local/Ffuenf/StockControl"
      ],
      [
        "app/etc/modules/Ffuenf_StockControl.xml",
        "app/etc/modules/Ffuenf_StockControl.xml"
      ]
    ]
  }
}