{
  "name": "Ffuenf/Ffuenf_StockControl",
  "license": "MIT",
  "description": "This extension provides an easy-to-use admin grid displaying the important stock information for your store.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    },
    {
      "name": "Adam Moss"
    }
  ],
  "type": "magento-module",
  "require": {
    "aoepeople/composer-installers": "*",
    "colinmollenhour/modman": "*"
  },
  "require-dev": {
    "kirchbergerknorr/magento": "1.9.1.*",
    "ecomdev/ecomdev_phpunit": "*",
    "phpunit/phpunit": "4.1.*"
  },
  "autoload-dev": {
    "classmap": ["vendor/kirchbergerknorr/magento/app", "vendor/kirchbergerknorr/magento/lib"]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    },
    {
      "type": "composer",
      "url": "http://packages.kirchbergerknorr.de"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "auto-append-gitignore": true,
    "magento-force": true
  }
}