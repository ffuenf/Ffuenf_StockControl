{
  "name": "Ffuenf/Ffuenf_StockControl",
  "license": "MIT",
  "description": "This extension provides an easy-to-use admin grid displaying the important stock information for your store.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    },
    {
      "name": "Adam Moss"
    }
  ],
  "type": "magento-module",
  "require": {
    "aoepeople/composer-installers": "*",
    "colinmollenhour/modman": "*"
  },
  "require-dev": {
    "magento/magento": "1.9",
    "ecomdev/ecomdev_phpunit": "*",
    "phpunit/phpunit": "4.1.*"
  },
  "autoload-dev": {
    "classmap": ["app", "skin", "vendor/magento/magento/app", "vendor/magento/magento/lib"]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    },
    {
      "type": "package",
      "package": {
        "name": "magento/magento",
        "version": "1.9",
        "source": {
          "url": "http://svn.magentocommerce.com/source",
          "type": "svn",
          "reference": "branches/1.9"
        },
        "classmap": ["app/code/local", "app/code/community", "app/code/core", "lib"]
      }
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/"
  }
}